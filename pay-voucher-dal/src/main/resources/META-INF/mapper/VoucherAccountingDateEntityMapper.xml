<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dream.pay.voucher.dao.VoucherAccountingDateDao">
    <resultMap id="BaseResultMap" type="com.dream.pay.voucher.model.VoucherAccountingDateEntity">
        <constructor>
            <arg column="last_accounting_date" jdbcType="VARCHAR" javaType="java.lang.String"/>
            <arg column="accounting_date" jdbcType="VARCHAR" javaType="java.lang.String"/>
        </constructor>
    </resultMap>
    <sql id="Base_Column_List">
        last_accounting_date, accounting_date
    </sql>
    <update id="updateAccountingDate">
        update voucher_accounting_date
        <set>
            last_accounting_date = #{lastAccountingDate,jdbcType=VARCHAR},
            accounting_date = #{accountingDate,jdbcType=VARCHAR},
        </set>
    </update>
    <select id="selectAccountingDate" resultType="java.lang.String">
        select accounting_date from voucher_accounting_date;
    </select>
</mapper>